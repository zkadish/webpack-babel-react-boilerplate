{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "rules": {
      "no-extraneous-dependencies": 0,
      "import/no-extraneous-dependencies": 0,
      "import/extensions": 0,
      "import/no-unresolved": 0,
      "react/jsx-filename-extension": 0
    },
    "env": {
      "browser": true,
      "node": true
    }
}
